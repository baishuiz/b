<!doctype html>
<html>
<head>
  <meta charset="utf-8">
  <title>Jasmine Spec Runner</title>
  <link rel="shortcut icon" type="image/png" href="<%= temp %>/jasmine_favicon.png">
<% css.forEach(function(style){ %>
  <link rel="stylesheet" type="text/css" href="<%= style %>">
<% }) %>

</head>
<body>
<style>
viewport view {display:none}
viewport view[active="true"]{display:block}
</style>


<viewport main="true">
  <view name="page_default" >
    <p>Hello</p>
    <span class="flower" attr="{{attr}}">{{flower === attr ? '' : flower}} - {{attr}}</span>
    <span class="flower_A" attr="{{attr}}">{{1-1==0 ? citySH : cityTL}}</span>
    <span class="flower_B" attr="{{attr}}">{{true ? 'tie ling' : citySH}}</span>
    <span class="flower_C" >{{obj.name}}{{obj.age}}</span>
  </view>
  <view name="page_list">
    <p>Hello</p>
    <span class="flower" attr="{{attr}}">{{flower}}</span>
  </view>

  <view name="page_url_change_before"></view>
  <view name="page_url_change_after"></view>
  <view name="page_detail">
    <span>{{parentValue}}</span>
    <view name="page_detail_subview">
      <p class="text">{{parentValue}} - {{subValue}}</p>
    </view>
    <h1>{{parentValue}}</h1>
  </view>

  <view name="page_list_hash">
    <p>Hello</p>
    <span class="flower" attr="{{attr}}">{{flower}}</span>
  </view>
  <view name="page_detail_hash">
    <p>Hello page_detail_hash</p>
  </view>

  <view name="page_scope_change">
    <div class="form-group col-sm-6">
      <div class="col-sm-8">
        <span class="obj" b-show="validateResult" style="display: none;"></span>
        <span class="help-block" b-repeat="m in validateResult.branch">
          {{m.msg}}
        </span>
      </div>
    </div>

    <span class="address">{{a.b.address}}</span>
  </view>
  <view name="page_booking"></view>
  <view name="page_order"></view>
  <view name="page_service"></view>
  <view name="page_service_change">
    <p class="city">City = {{city}}</p>
  </view>
  <view name="page_service_cache"></view>
  <view name="page_service_middleware_before"></view>
  <view name="page_service_middleware_after"></view>
  <view name="page_service_timeout"></view>
  <view name="page_service_queue"></view>
  <view name="page_event">
    <a href="javascript:void(0)" class="btn" b-event="click clickHandle('Hello'); test testHandle()">{{helloEvent.event.msg}}</a>
  </view>
  <view name="page_event_param">
    <a href="javascript:void(0)" class="btn" b-event="click clickHandle('{{hi}}', '{{scopeParam}}', 'Hello(World)')">Hello Event</a>
  </view>
  <view name="page_event_multiple">
    <a href="javascript:void(0)" class="btn" b-event="click clickHandle('click'); focus focusHandle('focus')">Hello Event</a>
  </view>

  <view name="remote_page_partialTestA">
    <p>{{pageName}}</p>
    <ul><li b-repeat="item in list">{{item.name}}</li></ul>
    <view b-scope-key="remote_page_partialTestA_partialPage" name="partialPage">
      <a href="javascript:void(0)" class="partialBtn" b-event="click partialViewEventTest('AOK')">{{pageName}}{{eventMsg}}</a>
    </view>
  </view>

  <view name="remote_page_partialTestB">
    <p>{{pageName}}</p>
    <view b-scope-key="remote_page_partialTestB_partialPage" name="partialPage">
      <a href="javascript:void(0)" class="partialBtn" b-event="click partialViewEventTest('BOK')">{{pageName}}{{eventMsg}}</a>
    </view>
  </view>

  <view name="page_repeat">
    <p>{{notice}}</p>
    <ul>
      <li b-repeat="item in list" name="{{item.name}}{{notice}}">
                {{ item.name }}
              </li>
    </ul>
    <div class='listB'>
      <span b-repeat="item in a.b.list">{{item.name}}</span>
    </div>
    <div></div>
  </view>


    <view name="page_repeat_clear">
      <p>{{notice}}</p>
      <ul>
         <li b-repeat="item in list">
              <button b-repeat="innerItem in item.innerList">{{ innerItem.name }}{{notice}}</button>
         </li>
      </ul>
      <div></div>
    </view>

    <view name="page_repeat_changeData">
         <p>{{notice}}</p>
         <ul>
            <li b-repeat="item in list">
                 <button>{{ item.attr }}{{notice}},{{ item.attr }}</button>
                 <button>{{ item.obj.name }}{{notice}},{{ item.obj.name }}</button>
                 <button b-repeat="innerItem in item.innerList">{{ innerItem.a.b.name }}{{notice}},{{ innerItem.a.b.name }}</button>
            </li>
         </ul>
         <div></div>
    </view>

    <view name="page_repeat_twiceChangeData">
         <p>{{notice}}</p>
         <ul>
            <li b-repeat="item in list">
                 <button>{{ item.attr }}{{notice}},{{ item.attr }}</button>
                 <button>{{ item.obj.name }}{{notice}},{{ item.obj.name }}</button>
                 <button b-repeat="innerItem in item.innerList">{{ innerItem.a.b.name }}{{notice}},{{ innerItem.a.b.name }}</button>
            </li>
         </ul>
         <div></div>
    </view>

    <view name="page_repeat_cutData">
       <p>{{notice}}</p>
       <ul>
          <li b-repeat="item in list">
               <button>{{ item.attr }}{{notice}},{{ item.attr }}</button>
               <button>{{ item.obj.name }}{{notice}},{{ item.obj.name }}</button>
               <button b-repeat="innerItem in item.innerList">{{ innerItem.a.b.name }}{{notice}},{{ innerItem.a.b.name }}</button>
          </li>
       </ul>
       <div></div>
    </view>

    <view name="page_repeat_multiple_call">
           <p>{{notice}}</p>
           <ul>
              <li b-repeat="item in list">
                   <button>{{ item.attr }}{{notice}},{{ item.attr }}</button>
                   <button>{{ item.obj.name }}{{notice}},{{ item.obj.name }}</button>
                   <button b-repeat="innerItem in item.innerList">{{ innerItem.a.b.name }}{{notice}},{{ innerItem.a.b.name }}</button>
              </li>
           </ul>
           <ul>
             <li b-repeat="item in list">
                  <button>{{ item.attr }}{{notice}},{{ item.attr }}</button>
                  <button>{{ item.obj.name }}{{notice}},{{ item.obj.name }}</button>
                  <button b-repeat="innerItem in item.innerList">{{ innerItem.a.b.name }}{{notice}},{{ innerItem.a.b.name }}</button>
             </li>
          </ul>
           <div></div>
        </view>

  <view name="page_repeat_change_by_unshift">
    <p>{{notice}}</p>
    <ul>
      <li b-repeat="item in list" name="{{item.name}}{{notice}}">
        {{ item.name }}
      </li>
    </ul>
    <div class='listB'>
      <span b-repeat="item in a.b.list">{{item.name}}</span>
    </div>
    <div></div>
  </view>

  <view name="page_repeat_with_b-model">
    <div class='listB' >
      <input b-repeat="item in a.b.list" type="text" b-model="item.name" />
    </div>
    <div></div>
  </view>
  <view name="page_repeat_with_b-show">
    <div class='listB' >
      <input b-repeat="item in a.b.list" b-show="item.status" type="text" b-model="item.name" />
    </div>
    <div></div>
  </view>
  <view name="page_repeat_subElement_with_b-show">
    <div class='listB' b-repeat="item in a.b.list" >
      <input  b-show="item.status" type="text" b-model="item.name" />
    </div>
    <div></div>
  </view>

    <view name="page_repeat_with_b-exist">
    <div class='listB' >
      <input b-repeat="item in a.b.list" b-exist="item.status" type="text" b-model="item.name" />
    </div>
    <div></div>
  </view>






  <view name="page_repeat_subElement_with_b-exist">
    <div class='listB' b-repeat="item in a.b.list" >
      <input  b-exist="item.status" type="text" b-model="item.name" />
    </div>
    <div></div>
  </view>

<view name="page_repeat_subElement_with_b-repeat">
    <div class='listC' b-repeat="item in list" >
        <div class='spanParent' b-exist="item.showStatus_1">
            <span class='span'>{{item.name}}</span>
            <div class='btnsParent'>
                <div class='btns' b-repeat="div1 in item.div1" b-exist="div1.status">
                   <span>{{div1.name}}</span>
                   <span>{{div1.name}}</span>
                   <span>{{div1.name}}</span>
                   <span>{{div1.name}}</span>
                   <button>{{div1.name}}</button>
                </div>
            </div>
        </div>

        <div class='spanParent' b-exist="item.showStatus_2">
            <span class='span'>{{item.name}}</span>
            <div class='btnsParent'>
                <div class='btns' b-repeat="div1 in item.div1" b-exist="div1.status">
                   <span>{{div1.name}}</span>
                   <span>{{div1.name}}</span>
                   <span>{{div1.name}}</span>
                   <span>{{div1.name}}</span>
                   <button>{{div1.name}}</button>
                </div>
            </div>
        </div>


    </div>
    <div></div>
  </view>






  <view name="page_repeat_in_repeat">
    <div class="list1" b-repeat="item1 in list1">
      <div class="list2" b-repeat="item2 in list2">
        {{item1.name}}{{item2.name}}
      </div>
    </div>
  </view>
  <view name="page_repeat_in_own_repeat">
    <div class="list" b-repeat="item in list">
      <div class="sub-list" b-repeat="subItem in item.subList">
        {{item.name}} {{subItem.name}}
      </div>
    </div>
  </view>
  <view name="page_repeat_nearby_repeat">
    <ul class="list1">
      <li b-repeat="item1 in list1">{{item1.name}}</li>
    </ul>
    <ul class="list2">
      <li b-repeat="item2 in list2">{{item2.name}}</li>
    </ul>
  </view>
  <view name="page_repeat_table">
    <table>
      <tr>
        <th b-repeat="header in headers">
          {{header.text}}
        </th>
      </tr>
      <tr b-repeat="item in items" class="tr_{{item.text}}">
        <td b-repeat="td in item.list">
          {{item.text}}{{td.text}}
        </td>
      </tr>
    </table>
  </view>
  <view name="page_repeat_expression_in_repeat">
    <ul class="list">
      <li b-repeat="item in list">{{item.count === pager.index ? item.count : 1}}</li>
    </ul>
  </view>
  <view name="page_repeat_with_event_outer">
    <ul>
      <li b-repeat="item in list" b-event="click clickHandle('good')">{{item.name}}</li>
    </ul>
    <div class="listB">
      <span b-repeat="item in a.b.list">{{item.name}}</span>
    </div>
  </view>
  <view name="page_repeat_with_event_inner">
    <ul>
      <li b-repeat="item in lista">
        <a href="javascript:void(0)" b-event="click clickHandle({{item.$index}}, {{item.$index}}, {{item.$index}})">{{item.name}}</a>
      </li>
    </ul>
  </view>
  <view name="page_repeat_with_data">
    <p class="title" b-event="click titleClickHandle()">{{notice}}</p>
    <ul>
      <li b-repeat="item in list">
        <a href="javascript:void(0)" b-event="click itemClickHandle({{item.$index}})">{{item.name}}</a>
      </li>
    </ul>
  </view>
  <view name="page_repeat_in_repeat_event">
    <p class="title">{{notice}}</p>
    <div>
      <div class="list1" b-repeat="item1 in list1">
        <div class="list2" b-repeat="item2 in list2">
          <a href="javascript:void(0)" b-event="click clickHandle({{item2.$index}})">{{item1.name}}{{item2.name}}</a>
        </div>
      </div>
    </div>
  </view>



  <view name="page_repeat_with_big_data">
      <p class="title">{{notice}}</p>
      <ul>
        <li b-repeat="man in list" class="{{man.sex === 'male' ? 'male' : 'female'}} {{man.age}} {{man.isHandsome ? 'handsome' : ''}}" b-show="man.hasBoyFriend">
            <span>姓名:{{man.name}};</span><p>住址:{{man.address}};工作:{{man.work}};是否单身:{{man.hasGirlFriend ? '是' : '否'}};身高:{{man.high/100}}米;体重:{{man.weight}}kg;备注:{{man.remark}}</p>
        </li>
      </ul>
  </view>






  <view name="page_model_bind">
    <input b-model="username" type="text" class="username"></input>
    <input b-model="checked" type="checkbox" class="checkbox"></input>
  </view>
  <view name="page_model_input">
    <input b-event="input inputHandle()" b-model="username" type="text" class="username"></input>
  </view>
  <view name="page_model_checkbox">
    <input b-event="click clickHandle()" b-model="check" type="checkbox" class="checktest"></input>
  </view>
  <view name="page_model_checkboxGroup">
      <input  b-model="checks" value="1" type="checkbox" name="checktest" class="checktest"></input>
      <input  b-model="checks" value="2" type="checkbox" name="checktest" class="checktest"></input>
      <input  b-model="checks" value="3" type="checkbox" name="checktest" class="checktest"></input>
      <input  b-model="checks" value="4" type="checkbox" name="checktest" class="checktest"></input>
      <input  b-model="checksB" value="a1" type="checkbox" name="checktestB" class="checktestB"></input>
      <input  b-model="checksB" value="a2" type="checkbox" name="checktestB" class="checktestB"></input>
      <input  b-model="checksB" value="a3" type="checkbox" name="checktestB" class="checktestB"></input>
      <input  b-model="checksB" value="a4" type="checkbox" name="checktestB" class="checktestB"></input>
  </view>
  <view name="page_model_radio">
    <input  b-model="check" type="radio" class="checktest" name="radioTest" value="on" />
    <input  b-model="check" type="radio" class="checktest" name="radioTest" value="off" />
  </view>
  <view name="page_b-show">
    <style>#page_b-show_table { display: none; }</style>
    <p class="p1" b-show="logined" b-event="click switchi()"></p>
    <table b-show="logined" id="page_b-show_table"></table>
    <ul>
      <li b-show="logined" id="page_b-show_list_item"></li>
    </ul>
    <p class="p2" b-show="count > 1"></p>
    <p class="p3" b-show="node.num === 1"></p>
    <p class="p4" b-show="node.ppp"></p>
  </view>
  <view name="page_b-exist">
      <style>#page_b-exist_table { display: none; }</style>
      <p class="p1" b-exist="logined" b-event="click switchi()"></p>
      <table b-exist="logined" id="page_b-exist_table"></table>
      <ul>
        <li b-exist="logined" id="page_b-exist_list_item"></li>
      </ul>
      <p class="p2" b-exist="count > 1"></p>
      <p class="p3" b-exist="node.num"></p>
      <p class="p4" b-exist="node.ppp"></p>
    </view>

  <view name="page_b-style">
    <style>#page_b-style_table { display: none; }</style>
    <p b-style="display:{{logined}}" b-event="click switchi()"></p>
    <table b-style="display:{{logined}}" id="page_b-style_table"></table>
    <ul>
      <li b-style="display:{{logined}}" id="page_b-style_list_item"></li>
    </ul>
  </view>

  <view name="page_select-option">
     <select b-model="data.cityId" name="numSelector" >
        <option value="">请选择</option>
        <option b-repeat="city in cityList" value="{{city.cityId}}">{{city.cityName}}</option>
     </select>
  </view>

  <view name="page_b-property">
    <input b-event="click switchi()" b-property="{readOnly: a.b.readonly, disabled: a.b.disable}" id="page_b-property-input" />

    <ul>
      <li b-repeat="item in list">
        <input b-property="{readOnly: item.status.readonly, disabled: item.status.disable}"/>
      </li>
    </ul>
  </view>

  <view name="page_service_event">
      <p b-repeat="city in hot.cities">{{city}}</p>
      <span>{{error}}</span>
  </view>
</viewport>

<viewport class=".footer">
    <view name="side_footer">
        {{hi}}{{$service.list.data.msg}}
    </view>
</viewport>

<% with (scripts) { %>
  <% [].concat(polyfills, jasmine, boot, vendor, helpers, src).forEach(function(script){ %>
  <script src="<%= script %>"></script>
  <% }) %>
<% }; %>

<script>
    window.host = '127.0.0.1:8000';

    ;(function(){
      b.router.set({
        rule : '/index/:info',
        viewName : 'page_default'
      })

      b.router.set({
        rule : '/list/:city',
        viewName : 'page_list'
      })

      b.router.set({
        rule : '/detail/:roomid',
        viewName : 'page_detail'
      })

      b.router.set({
         rule : '/listHash/:city',
         viewName : 'page_list_hash'
      })

      b.router.set({
         rule : '/detailHash/:roomid',
         viewName : 'page_detail_hash'
      })

      b.router.set({
        rule : '/booking',
        viewName : 'page_booking'
      })

      b.router.set({
        rule : '/order',
        viewName : 'page_order'
      })

      b.router.set({
        rule : '/remote_page_map',
        viewName : 'remote_page_map'
      })

      b.router.set({
        rule : '/remote_page_list',
        viewName : 'page_order'
      })

      b.router.set({
        rule : '/remote_page_detail',
        viewName : 'page_order'
      })

      b.router.set({
        rule : '/remote_page_scope',
        viewName : 'remote_page_scope'
      })

      b.router.set({
        rule : '/page_url_change_before',
        viewName : 'page_url_change_before'
      });

      b.router.set({
        rule : '/page_url_change_after',
        viewName : 'page_url_change_after'
      });

      b.router.set({
        rule : '/remote_page_partialTestA',
        viewName : 'remote_page_partialTestA'
      });

      b.router.set({
        rule : '/remote_page_partialTestB',
        viewName : 'remote_page_partialTestB'
      });

    }());
</script>

<% with (scripts) { %>
  <% [].concat(specs,reporters).forEach(function(script){ %>
  <script src="<%= script %>"></script>
  <% }) %>
<% }; %>
</body>
</html>
